{
  "manifest_version": 2,

  "name": "A draw grad example",
  "description": "just a demo.",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/index.html"
  },

  "permissions": [
    "contextMenus",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],

  "web_accessible_resources": [
    "popup/inject.js"
  ],
  "content_scripts": [{
    "matches": ["https://*.huiyihuiying.com/*"],
    "js": ["plugin/entry.js"],
    "run_at": "document_end"
  }]

}